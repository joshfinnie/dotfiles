# My Aliases
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias mkdir='mkdir -p'

alias h='history'
alias which='type -a'
alias ..='cd ..'
alias ...='cd ../..'
alias ls='exa -lag --header'
alias ccd='clear && cd'

alias killpyc='find . -name \*.pyc -delete'

alias latex="docker run -v `pwd`:/tmp latex pdflatex"

alias vimflict="git diff --name-only | uniq | xargs vim"

alias exa="/opt/homebrew/bin/exa"

alias e="open -a /Applications/Emacs.app &"
alias v="nvim"
